<UserControl x:Class="WordMaster.TestFinishUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WordMaster"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Label FontSize="50" Content="Your score:" HorizontalAlignment="Center" Margin="0,115,0,0" VerticalAlignment="Top"/>
        <TextBlock x:Name="finishTestPoints" FontSize="20" TextAlignment="Center" HorizontalAlignment="Center" Text="TextBlock" TextWrapping="Wrap" VerticalAlignment="Top" Width="182" Margin="0,203,0,0"/>
        <Button Click="end_Click" FontSize="20" Content="End Test" HorizontalAlignment="Center" Margin="0,269,0,0" VerticalAlignment="Top" Height="33" Width="136"/>

    </Grid>
</UserControl>
